# Ease of Use
alias sl="ls"
alias l="ls"
alias ll="ls -la"
alias reload="exec $SHELL -l"
alias rl="exec $SHELL -l"
alias :q=exit

# Shortcuts
alias h="cd ~"
alias dt="cd ~/Desktop"
alias dl="cd ~/Downloads"
alias doc="cd ~/Documents"
alias ws="cd ~/workspace"
alias cws="cd ~/catkin_ws"
alias gws="cd ~/go"

# Cheatsheets
alias i3cheatsheet='grep '^bind' .config/i3/config | cut -d " " -f 2- | column | less'
